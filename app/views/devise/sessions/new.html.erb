
       <%= form_for(resource, as: resource_name, url: session_path(resource_name),:html => {:class => "form-horizontal col-md-offset-3"}) do |f| %>
            <div class="form-group col-md-offset-6">
              <%= f.label :email,:class => "col-sm-2 control-label" %>
              <div class="col-sm-10 ">
                <%= f.email_field :email, autofocus: true,:class => "col-md-4" %>
               </div> 
            </div>

            <div class="form-group col-md-offset-6">
              <%= f.label :password,:class => "col-md-2 control-label" %>
              <div class="col-sm-10">
              <%= f.password_field :password, autocomplete: "off",:class => "col-md-4" %>
              </div> 
            </div>

            <!--<% if devise_mapping.rememberable? -%>
              <div class="form-group col-md-offset-8">
                <%= f.check_box :remember_me,:class => "col-md-2 control-label" %>
                <%= f.label :remember_me %>
              </div>
            <% end -%> -->

            <div class="actions col-md-offset-3">
              <%= f.submit "Log in",:class => "btn btn-md btn-primary" %>
            </div>
         <!--   <%= render "devise/shared/links" %> -->
          <% end %>


