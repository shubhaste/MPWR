
       <%= form_for(resource, as: resource_name, url: session_path(resource_name),:html => {:class => "form-horizontal col-md-offset-3"}) do |f| %>
            



    <div class="row">
      <div class="col-md-12">
        <div class="col-md-6 col-md-offset-1">
          <div class="panel panel-info">
            <div class="panel-heading" align="center"><h5><strong>Welcome to hawlahaguud Online Process</strong></h5></div>
              <div class="panel-body">
                <form role="form">
                  <div class="form-group">
                    <%= f.label :email,:class => "col-sm-2 control-label" %>                      
                       <%= f.email_field :email, autofocus: true,:class => "form-control",:placeholder => "enter email" %>
                  </div>
                  <div class="form-group">
                    <%= f.label :password,:class => "col-md-2 control-label" %>
                      
                       <%= f.password_field :password, autocomplete: "off",:class => "form-control",:placeholder => "enter password" %>
                  </div>
                    
                    <%= f.submit "Log in",:class => "btn btn-info col-md-12" %>
                </form>
              </div>
          </div>
        </div>
      </div>
    </div>

         <!--   <%= render "devise/shared/links" %> -->
          <% end %>


